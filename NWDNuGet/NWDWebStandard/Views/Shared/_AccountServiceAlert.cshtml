@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@using Microsoft.AspNetCore.Http
@using NWDFoundation.Models
@using NWDWebRuntime.Managers

@inject IHttpContextAccessor HttpContextAccessor

@foreach (NWDAccountService tService in NWDAccountServiceWebManager.GetServices(HttpContextAccessor.HttpContext))
{
    if (tService.IsDateActive())
    {
        if (string.IsNullOrEmpty(tService.Message) == false)
        {
            <div class="alert alert-@tService.MessageStyle.ToString().ToLower()">
                @tService.Message
            </div>
        }
    }
}